{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": []
  },
  "lib": [
    "es2016",
    "dom"
  ],
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ],
  "paths": {
    "@app/*": [
      "app/"
    ],
    "@environment/*": [
      "environments/*"
    ],
    "@services/*": [
      "app/services/*"
    ],
    "@pages/*": [
      "app/pages/*"
    ],
    "@models/*": [
      "app/models/*"
    ],
    "@components/*": [
      "app/components/*"
    ]
  }
}